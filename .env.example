# Gitea Configuration
GITEA_BASE_URL=http://localhost:3000
GITEA_TOKEN=your-gitea-api-token-here

# Repository Configuration
REPO_OWNER=myorg
REPO_NAME=myrepo

# Agent Configuration
AGENT_TYPE=claude-local  # Options: claude-local, claude-api, openai
AGENT_MODEL=claude-sonnet-4.5
CLAUDE_API_KEY=  # Required if using claude-api
OPENAI_API_KEY=  # Required if using openai

# Workflow Configuration
MAX_CONCURRENT=3
BRANCHING_STRATEGY=per-agent  # Options: per-agent, shared

# Optional: Daemon polling interval (seconds)
DAEMON_INTERVAL=60
